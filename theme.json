{
  "version": 2,
  "customTemplates": [
    {
      "name": "blank",
      "title": "Blank",
      "postTypes": [
        "page",
        "post"
      ]
    }
  ],
  "templateParts": [
    {
      "name": "header",
      "area": "header"
    },
    {
      "name": "content-archive",
      "area": "uncategorized"
    },
    {
      "name": "content-page",
      "area": "uncategorized"
    },
    {
      "name": "footer",
      "area": "footer"
    }
  ],
  "settings": {
    "appearanceTools": true,
    "useRootPaddingAwareAlignments": true,
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "color": {
      "background": true,
      "custom": true,
      "customDuotone": true,
      "customGradient": true,
      "link": true,
      "text": true,
      "palette": [
        {
          "slug": "black",
          "color": "#000000",
          "name": "Black"
        },
        {
          "slug": "white",
          "color": "#ffffff",
          "name": "White"
        },
        {
          "slug": "yellow",
          "color": "#ffdc00",
          "name": "Yellow"
        },
        {
          "slug": "orange",
          "color": "#FF5F1F",
          "name": "Orange"
        },
        {
          "slug": "amaranth",
          "color": "#e10f47",
          "name": "Amaranth"
        },
        {
          "slug": "burnt-teal",
          "color": "#108387",
          "name": "Burnt Teal"
        },
        {
          "slug": "purple",
          "color": "#7E21CD",
          "name": "Purple"
        },
        {
          "slug": "ultra-blue",
          "color": "#4927fc",
          "name": "Ultra Blue"
        }
      ],
      "gradients": [
        {
          "slug": "white-dotted-overlay",
          "gradient": "radial-gradient(circle at center, white 0.2rem, transparent 0)",
          "name": "White Dotted Overlay"
        },
        {
          "slug": "black-dotted-xs-overlay",
          "gradient": "radial-gradient(circle at center, black 0.15rem, transparent 0)",
          "name": "Small Black Dotted Overlay"
        },
        {
          "slug": "dark-diamonds-xs-overlay",
          "gradient": "radial-gradient(circle at center, black 2.1rem, transparent 0), radial-gradient(circle at center, black 2.1rem, transparent 0)",
          "name": "Dark Diamonds"
        },
        {
          "slug": "button-pattern-overlay",
          "gradient": "radial-gradient(circle at center, rgba(0,0,0,0.6) 2.45rem, transparent 0), radial-gradient(circle at center, black 2.05rem, transparent 0)",
          "name": "Button Pattern"
        },
        {
          "slug": "white-dotted-xs-overlay",
          "gradient": "radial-gradient(circle at center, white 0.135rem, transparent 0)",
          "name": "Small White Dotted Overlay"
        },
        {
          "slug": "black-dotted-overlay",
          "gradient": "radial-gradient(circle at center, black 0.125rem, transparent 0)",
          "name": "Small Black Dotted Overlay"
        }
      ]
    },
    "layout": {
      "contentSize": "var(--wp--custom--content--base)",
      "wideSize": "var(--wp--custom--content--wide)"
    },
    "spacing": {
      "blockGap": true,
      "margin": true,
      "padding": true,
      "units": [ "px", "em", "rem", "vh", "vw", "%" ],
      "spacingScale": {
        "operator": "*",
        "increment": 1.5,
        "steps": 7,
        "mediumStep": 1.5,
        "unit": "rem"
      }
    },
    "typography": {
      "customFontSize": true,
      "fontStyle": true,
      "fontWeight": true,
      "letterSpacing": true,
      "textDecoration": true,
      "lineHeight": true,
      "textTransform": true,
      "dropCap": true,
      "fontSizes": [
        {
          "size": ".9rem",
          "slug": "small",
          "name": "Small"
        },
        {
          "size": "1rem",
          "slug": "base",
          "name": "Base"
        },
        {
          "size": "1.225rem",
          "slug": "lead",
          "name": "Lead"
        },
        {
          "size": "1.425rem",
          "slug": "medium",
          "name": "Medium"
        },
        {
          "size": "1.75rem",
          "slug": "large",
          "name": "Large"
        },
        {
          "size": "clamp(1.75rem, 3vw, 2.25rem)",
          "slug": "x-large",
          "name": "Extra-Large"
        },
        {
          "size": "clamp(2.5rem, 4vw, 4rem)",
          "slug": "huge",
          "name": "Huge"
        }
      ],
      "fontFamilies": [
        {
          "fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
          "name": "System Font",
          "slug": "overpass"
        },
        {
          "fontFamily": "'Overpass', sans-serif, serif",
          "slug": "overpass",
          "name": "Overpass",
          "fontFace": [
            {
              "fontFamily": "Overpass",
              "fontStretch": "auto",
              "fontStyle": "auto",
              "fontWeight": "100 200 300 500 600 900",
              "fontDisplay": "swap",
              "src": [
                "file:./fonts/Overpass/Overpass-VariableFont_wght.woff2"
              ]
            },
            {
              "fontFamily": "Overpass",
              "fontStretch": "auto",
              "fontStyle": "italic",
              "fontWeight": "100 200 300 500 600 900",
              "fontDisplay": "swap",
              "src": [
                "file:./fonts/Overpass/Overpass-Italic-VariableFont_wght.woff2"
              ]
            }
          ]
        },
        {
          "fontFamily": "\"Source Serif Pro\", serif",
          "name": "Source Serif Pro",
          "slug": "source-serif-pro"
        }
      ]
    },
    "custom": {
      "border-radius": "0.75rem",
      "bg-opacity": 0.5,
      "font-size": {
        "root": "16px",
        "root-xs": "14px"
      },
      "line-height": {
        "normal": 1.575,
        "heading": 1.3,
        "page-title": 1.1
      },
      "content": {
        "base": "980px",
        "wide": "1120px"
      },
      "spacing": {
        "unit": "1rem",
        "horizontal": "1.5rem",
        "vertical": "1rem"
      },
      "font-weight":{
        "light": "300",
        "normal": "normal",
        "bold": "600"
      }
    },
    "blocks": {
      "core/group": {
        "spacing": {
          "padding": true,
          "margin": true,
          "blockGap": true
        }
      },
      "core/buttons": {
      },
      "core/cover": {
        "spacing": {
          "margin": true,
          "blockGap": true
        }
      },
      "core/heading": {
        "spacing": {
          "margin": true,
          "padding": true
        }
      },
      "core/post-content": {
        "spacing": {
          "blockGap": true,
          "padding": true
        }
      }
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--black)",
      "text": "var(--wp--preset--color--white)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--overpass)",
      "lineHeight": "var(--wp--custom--line-height--normal)",
      "fontSize": "var(--wp--preset--font-size--medium)"
    },
    "spacing": {
      "blockGap": "1.3rem"
    },
    "elements": {
      "link": {
        "color": {
          "text": "inherit"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large)",
          "lineHeight": "var(--wp--custom--line-height--page-title)",
          "fontWeight": "var(--wp--custom--font-weight--normal)",
          "fontFamily": "var(--wp--preset--font-family--overpass)"
        },
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "var(--wp--style--block-gap)"
          }
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large)",
          "lineHeight": "var(--wp--custom--line-height--heading)",
          "fontWeight": "var(--wp--custom--font-weight--normal)",
          "fontFamily": "var(--wp--preset--font-family--overpass)"
        },
        "spacing": {
          "margin": {
            "top": "var(--wp--style--block-gap)",
            "bottom": "0"
          }
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "lineHeight": "var(--wp--custom--line-height--heading)",
          "fontWeight": "var(--wp--custom--font-weight--normal)",
          "fontFamily": "var(--wp--preset--font-family--overpass)"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--lead)",
          "lineHeight": "var(--wp--custom--line-height--heading)",
          "fontFamily": "var(--wp--preset--font-family--overpass)"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--normal)",
          "lineHeight": "var(--wp--custom--line-height--heading)",
          "fontFamily": "var(--wp--preset--font-family--overpass)"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--extra-small)",
          "lineHeight": "var(--wp--custom--line-height--heading)",
          "fontFamily": "var(--wp--preset--font-family--overpass)"
        }
      }
    },
    "blocks": {
      "core/list-item": {
        "typography": {
          "lineHeight": "var(--wp--custom--line-height--base, 1.5)"
        },
        "spacing": {
          "margin": {
            "bottom": "var(--wp--preset--spacing--30)"
          }
        }
      },
      "core/heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--hepta-slab)",
          "lineHeight": "1.15"
        }
      },
      "core/paragraph": {
        "spacing": {
          "margin": {
            "bottom": "1rem"
          }
        }
      },
      "core/post-content": {
        "spacing": {
          "blockGap": "1.3rem",
          "padding": {
            "left": "var(--wp--preset--spacing--60)",
            "right": "var(--wp--preset--spacing--60)"
          }
        }
      },
      "core/template-part": {
        "spacing": {
          "blockGap": "0",
          "margin": {
            "top": "0"
          }
        }
      },
      "core/columns": {
        "spacing": {
          "blockGap": "2rem"
        }
      },
      "core/group": {
        "spacing": {
          "blockGap": "1.5rem",
          "padding": {
            "top": "0",
            "right": "0",
            "bottom": "0",
            "left": "0"
          }
        },
        "border": {
          "radius": "var(--wp--custom--border-radius, 0)"
        }
      },
      "core/buttons": {
        "spacing": {
          "blockGap": "2rem"
        }
      },
      "core/cover": {
        "spacing": {
          "blockGap": "1rem",
          "margin": {
            "top": "0",
            "right": "0",
            "bottom": "0",
            "left": "0"
          },
          "padding": {
            "top": "2rem",
            "right": "2rem",
            "bottom": "2rem",
            "left": "2rem"
          }
        }
      },
      "core/site-tagline": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "lineHeight": "1.4"
        }
      },
      "core/post-author": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--extra-small)",
          "lineHeight": "var(--wp--custom--line-height--body)"
        }
      },
      "core/post-date": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--extra-small)",
          "lineHeight": "var(--wp--custom--line-height--body)"
        }
      },
      "core/site-title": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "textTransform": "uppercase",
          "fontWeight": "var(--wp--custom--font-weight--normal)"
        }
      }
    }
  }
}
