{
  "version": 2,
  "customTemplates": [
    {
      "name": "blank",
      "title": "Blank",
      "postTypes": [
        "page",
        "post"
      ]
    }
  ],
  "templateParts": [
    {
      "name": "header",
      "area": "header"
    },
    {
      "name": "content-archive",
      "area": "content"
    },
    {
      "name": "content-page",
      "area": "content"
    },
    {
      "name": "footer",
      "area": "footer"
    }
  ],
  "settings": {
    "useRootPaddingAwareAlignments": true,
    "appearanceTools": true,
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "color": {
      "background": true,
      "custom": true,
      "customDuotone": true,
      "customGradient": true,
      "link": true,
      "text": true,
      "palette": [
        {
          "slug": "black",
          "color": "#000000",
          "name": "Black"
        },
        {
          "slug": "white",
          "color": "#ffffff",
          "name": "White"
        },
        {
          "slug": "yellow",
          "color": "color(display-p3 1 0.87 0)",
          "name": "Yellow"
        },
        {
          "slug": "orange",
          "color": "#FF5F1F",
          "name": "Orange"
        },
        {
          "slug": "solid-amaranth",
          "color": "color(display-p3 0.88 0.05 0.28)",
          "name": "Amaranth"
        },
        {
          "slug": "burnt-teal",
          "color": "#108387",
          "name": "Burnt Teal"
        },
        {
          "slug": "purple",
          "color": "#7E21CD",
          "name": "Purple"
        },
        {
          "slug": "ultra-blue",
          "color": "color(display-p3 0.3 0 0.99)",
          "name": "Ultra Blue"
        }
      ],
      "gradients": [
        {
          "slug": "amaranth-to-yellow",
          "gradient": "linear-gradient(168deg, var(--wp--preset--color--solid-amaranth) 0%, var(--wp--preset--color--orange) 63%, var(--wp--preset--color--yellow) 100%)",
          "name": "Amaranth to Yellow"
        }
      ]
    },
    "layout": {
      "contentSize": "980px",
      "wideSize": "1224px"
    },
    "shadow": {
      "defaultPresets": false,
      "presets": [
        {
          "name": "Natural",
          "slug": "natural",
          "shadow": "6px 6px 9px rgba(0, 0, 0, 0.2)"
        },
        {
          "name": "Deep",
          "slug": "deep",
          "shadow": "12px 12px 50px rgba(0, 0, 0, 0.4)"
        },
        {
          "name": "Sharp",
          "slug": "sharp",
          "shadow": "6px 6px 0px rgba(0, 0, 0, 0.2)"
        },
        {
          "name": "Outlined",
          "slug": "outlined",
          "shadow": "6px 6px 0px -3px var(--wp--preset--color--solid-yellow), 6px 6px rgba(0, 0, 0, 1)"
        },
        {
          "name": "Crisp",
          "slug": "crisp",
          "shadow": "6px 6px 0px rgba(0, 0, 0, 1)"
        },
        {
          "name": "Burnt",
          "slug": "crisp",
          "shadow": "6px 6px 0px var(--wp--preset--color--solid-amaranth)"
        }
      ]
    },
    "spacing": {
      "blockGap": true,
      "margin": true,
      "padding": true,
      "units": [ "px", "em", "rem", "vh", "vw", "%" ]
    },
    "typography": {
      "customFontSize": true,
      "fontStyle": true,
      "fontWeight": true,
      "letterSpacing": true,
      "textDecoration": true,
      "lineHeight": true,
      "textTransform": true,
      "dropCap": true,
      "fontSizes": [
        {
          "size": "0.85rem",
          "name": "Extra Small",
          "slug": "extra-small"
        },
        {
          "size": "1rem",
          "slug": "base",
          "name": "Base"
        },
        {
          "size": "1.15rem",
          "slug": "lead",
          "name": "Lead"
        },
        {
          "size": "1.4rem",
          "slug": "medium"
        },
        {
          "size": "1.75rem",
          "name": "Large",
          "slug": "large"
        },
        {
          "size": "2.2rem",
          "name": "Extra Large",
          "slug": "x-large"
        },
        {
          "size": "clamp(2.6rem, 3vw, 3.8rem)",
          "name": "Display",
          "slug": "display"
        },
        {
          "size": "clamp(3.5rem, 4.5vw, 5rem)",
          "name": "Extra Display",
          "slug": "x-display"
        },
        {
          "size": "clamp(4.5rem, 5vw, 6rem)",
          "name": "Huge",
          "slug": "huge"
        }
      ],
      "fontFamilies": [
        {
          "fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
          "name": "System Font",
          "slug": "system-font"
        },
        {
          "fontFamily": "'Hepta Slab', serif",
          "slug": "hepta-slab",
          "name": "Hepta Slab",
          "fontFace": [
            {
              "fontFamily": "Hepta Slab",
              "fontStretch": "auto",
              "fontStyle": "auto",
              "fontWeight": "100 200 300 500 600 900",
              "fontDisplay": "swap",
              "src": [
                "file:./fonts/Hepta_Slab/HeptaSlab-VariableFont_wght.woff2"
              ]
            }
          ]
        },
        {
          "fontFamily": "'Gantari', sans-serif",
          "slug": "gantari",
          "name": "Gantari",
          "fontFace": [
            {
              "fontFamily": "Gantari",
              "fontStretch": "auto",
              "fontStyle": "normal",
              "fontWeight": "100 300 400 500 600 700",
              "fontDisplay": "swap",
              "src": [
                "file:./fonts/Gantari/Gantari-VariableFont_wght.woff2"
              ]
            },
            {
              "fontFamily": "Gantari",
              "fontStretch": "auto",
              "fontStyle": "italic",
              "fontWeight": "100 300 400 500 600 700",
              "fontDisplay": "swap",
              "src": [
                "file:./fonts/Gantari/Gantari-Italic-VariableFont_wght.woff2"
              ]
            }
          ]
        },
        {
          "fontFamily": "'Instrument Sans', sans-serif",
          "slug": "instrument-sans",
          "name": "Instrument Sans",
          "fontFace": [
            {
              "fontFamily": "Instrument Sans",
              "fontStretch": "auto",
              "fontStyle": "normal",
              "fontWeight": "400 500 600 700",
              "fontDisplay": "swap",
              "src": [
                "file:./fonts/Instrument_Sans/InstrumentSans-VariableFont_wdth,wght.ttf"
              ]
            },
            {
              "fontFamily": "Instrument Sans",
              "fontStretch": "auto",
              "fontStyle": "italic",
              "fontWeight": "400 500 600 700",
              "fontDisplay": "swap",
              "src": [
                "file:./fonts/Instrument_Sans/InstrumentSans-Italic-VariableFont_wdth,wght.ttf"
              ]
            }
          ]
        },
        {
          "fontFamily": "'Readex Pro', sans-serif",
          "slug": "readex-pro",
          "name": "Readex Pro",
          "fontFace": [
            {
              "fontFamily": "Readex Pro",
              "fontStretch": "auto",
              "fontStyle": "normal",
              "fontWeight": "200 300 500 600",
              "src": [
                "file:./fonts/Readex_Pro/ReadexPro-VariableFont_HEXP,wght.ttf"
              ]
            }
          ]
        }
      ]
    },
    "custom": {
      "border-radius": "0.75rem",
      "bg-opacity": 0.5,
      "font-size": {
        "root": "16px",
        "root-xs": "14px"
      },
      "line-height": {
        "base": 1.4,
        "body": 1.7,
        "heading": 1.1,
        "page-title": 1.1
      },
      "content": {
        "base": "980px",
        "wide": "1520px"
      },
      "spacing": {
        "unit": "1rem",
        "horizontal": "1.5rem",
        "vertical": "1rem"
      },
      "font-weight":{
        "light": "300",
        "normal": "normal",
        "bold": "600"
      }
    },
    "blocks": {
      "core/group": {
        "spacing": {
          "padding": true,
          "margin": true,
          "blockGap": true
        }
      },
      "core/buttons": {
        "spacing": {
          "margin": true,
          "blockGap": true,
          "padding": true
        }
      },
      "core/cover": {
        "spacing": {
          "margin": true,
          "blockGap": true
        }
      },
      "core/heading": {
        "spacing": {
          "margin": true,
          "padding": true
        }
      },
      "core/post-content": {
        "spacing": {
          "blockGap": true,
          "padding": true
        }
      }
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--yellow)",
      "text": "var(--wp--preset--color--black)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--gantari)",
      "fontSize": "var(--wp--preset--font-size--base)",
      "lineHeight": "var(--wp--custom--line-height--base, 1.5)"
    },
    "spacing": {
      "blockGap": "1.3rem",
      "padding": {
        "top": "0",
        "right": "1.5rem",
        "bottom": "0",
        "left": "1.5rem"
      }
    },
    "elements": {
      "link": {
        "color": {
          "text": "inherit"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large)",
          "lineHeight": "var(--wp--custom--line-height--page-title)",
          "fontWeight": "var(--wp--custom--font-weight--normal)",
          "fontFamily": "var(--wp--preset--font-family--hepta-slab)",
          "letterSpacing": "-0.045em"
        },
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "var(--wp--style--block-gap)"
          }
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large)",
          "lineHeight": "var(--wp--custom--line-height--heading)",
          "fontWeight": "var(--wp--custom--font-weight--normal)",
          "fontFamily": "var(--wp--preset--font-family--hepta-slab)",
          "letterSpacing": "-0.045em"
        },
        "spacing": {
          "margin": {
            "top": "var(--wp--style--block-gap)",
            "bottom": "0"
          }
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "lineHeight": "var(--wp--custom--line-height--heading)",
          "fontWeight": "var(--wp--custom--font-weight--normal)",
          "fontFamily": "var(--wp--preset--font-family--hepta-slab)",
          "letterSpacing": "-0.045em"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--lead)",
          "lineHeight": "var(--wp--custom--line-height--heading)",
          "fontFamily": "var(--wp--preset--font-family--hepta-slab)",
          "letterSpacing": "-0.045em"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--normal)",
          "lineHeight": "var(--wp--custom--line-height--heading)",
          "fontFamily": "var(--wp--preset--font-family--hepta-slab)",
          "letterSpacing": "-0.045em"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--extra-small)",
          "lineHeight": "var(--wp--custom--line-height--heading)",
          "fontFamily": "var(--wp--preset--font-family--hepta-slab)",
          "letterSpacing": "-0.045em"
        }
      }
    },
    "blocks": {
      "core/post-content": {
        "spacing": {
          "blockGap": "1.3rem",
          "padding": {
            "left": "var(--wp--preset--spacing--60)",
            "right": "var(--wp--preset--spacing--60)"
          }
        }
      },
      "core/list-item": {
        "typography": {
          "lineHeight": "var(--wp--custom--line-height--base, 1.5)"
        },
        "spacing": {
          "margin": {
            "bottom": "var(--wp--preset--spacing--30)"
          }
        }
      },
      "core/heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--hepta-slab)",
          "lineHeight": "1.15"
        }
      },
      "core/paragraph": {
        "typography": {
          "lineHeight": "1.5"
        },
        "spacing": {
          "margin": {
            "bottom": "1rem"
          }
        }
      },
      "core/template-part": {
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "0"
          }
        }
      },
      "core/columns": {
        "spacing": {
          "blockGap": "2rem"
        }
      },
      "core/group": {
        "spacing": {
          "blockGap": "1.5rem",
          "padding": {
            "top": "0",
            "right": "0",
            "bottom": "0",
            "left": "0"
          }
        }
      },
      "core/buttons": {
        "spacing": {
          "blockGap": "2rem"
        }
      },
      "core/cover": {
        "spacing": {
          "blockGap": "1rem",
          "margin": {
            "top": "0",
            "right": "0",
            "bottom": "0",
            "left": "0"
          },
          "padding": {
            "top": "2rem",
            "right": "2rem",
            "bottom": "2rem",
            "left": "2rem"
          }
        }
      },
      "core/site-tagline": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "lineHeight": "1.4"
        }
      },
      "core/social-links": {
        "spacing": {
          "blockGap": "var(--wp--preset--spacing--40, 2rem)"
        },
        "color": {
          "text": "inherit"
        }
      },
      "core/site-title": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "textTransform": "uppercase",
          "fontWeight": "var(--wp--custom--font-weight--normal)"
        }
      }
    }
  }
}
